{
  "name": "playalong-project",
  "version": "1.0.0",
  "description": "Dynamic playalong generator written in TypeScript",
  "scripts": {
    "createScales": "npx ts-node scripts/createScales.ts",
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "rm -rf dist && npx webpack && npm run build && electron ./dist/main.js",
    "test": "nyc --reporter=html mocha -r ts-node/register test/**/*.ts"
  },
  "repository": "",
  "keywords": [
    "Electron",
    "typescript",
    "jazz",
    "music"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/mocha": "^8.0.3",
    "electron": "^9.0.4",
    "esm": "^3.2.25",
    "file-loader": "^6.1.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "tone": "^14.7.39",
    "webpack": "^4.44.1"
  }
}
